{
	"headers": [
		{
			"source": "/manifest.webmanifest",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/manifest+json"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		}
	]
}